  "description": "Live dynamic playlist for streaming",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/core/exports.js",
    "./core": "./src/core/exports.js",
    "./core/*": "./src/core/*",
    "./main": "./src/main/exports.js",
    "./main/*": "./src/main/*",
    "./media-server": "./src/media-server/exports.js",
    "./media-server/*": "./src/media-server/*",
    "./file-manager": "./src/file-manager/exports.js",
    "./file-manager/*": "./src/file-manager/*",
    "./utils": "./src/utils/exports.js",
    "./utils/*": "./src/utils/*",
    "./utils/dom": "./src/utils/dom/exports.js",
    "./utils/dom/ui": "./src/utils/dom/ui/exports.js",
    "./tools": "./tools/exports.js",
    "./tools/*": "./tools/*"
  },
  "scripts": {
    "test": "vitest",
    "build:dev": "node tools/cmd.js build --platform=windows",
    "build:dev+start-docker": "npm run build:dev && npm run docker:start",
    "build:prod": "node tools/cmd.js build --production",
    "electron:start": "node tools/cmd.js start",
    "electron:package": "node tools/cmd.js package",
    "electron:make": "node tools/cmd.js make",
    "build:prod+electron:package": "npm run build:prod && npm run electron:package",
    "build:prod+electron:make": "npm run build:prod && npm run electron:make",
    "start-external-stream": "tests\\start-external-stream.bat",
    "start-session-stream": "tests\\start-session-stream.bat",
    "start-live-stream": "tests\\start-live-stream.bat",
    "docker:start": "docker-compose up -d --build",
    "docker:stop": "docker-compose down"
  },
